	"scripts": {
		"dev": "cross-env NODE_OPTIONS=--trace-warnings next dev",
		"build": "next build --debug && echo 'Build completed in server mode'",
		"start": "next start",
		"lint": "next lint",
		"analyze": "ANALYZE=true next build",
		"build:prod": "NODE_ENV=production next build --debug",
		"build:analyze": "ANALYZE=true next build",
		"build:optimized": "./scripts/build-optimized.sh",
		"deploy:optimized": "./scripts/deploy-optimized.sh",
		"deploy:preview": "./scripts/deploy-optimized.sh --preview",
		"performance:report": "node -e \"console.log(require('./lib/metrics').getPerformanceReport())\"",
		"performance:cleanup": "node -e \"require('./lib/metrics').performanceMonitor.cleanup()\"",
		"performance:analyze": "node scripts/analyze-performance.js",
		"images:optimize": "node scripts/optimize-images.js",
		"images:optimize:aggressive": "node scripts/aggressive-image-optimization.js",
		"images:optimize:quick": "node scripts/quick-image-optimization.js",
		"images:optimize:covers": "node scripts/ultra-optimize-covers.js",
		"images:optimize:all": "node scripts/optimize-all-heavy-images.js",
		"bundle:optimize": "node scripts/optimize-bundle.js",
		"bundle:optimize:simple": "node scripts/simple-bundle-optimization.js",
		"images:update-refs": "node scripts/update-image-references.js",
		"optimize:cls": "node scripts/optimize-cls.js",
		"optimize:navbar": "node scripts/optimize-navbar-performance.js",
		"optimize:critical": "node scripts/optimize-critical-components.js",
		"optimize:motion": "node scripts/optimize-framer-motion.js",
		"performance:fix": "npm run optimize:navbar && npm run optimize:critical && npm run optimize:motion && npm run build",
		"performance:apply-all": "node scripts/apply-all-performance-fixes.js",
		"mobile:optimize": "node scripts/optimize-mobile-performance.js",
		"mobile:build": "npm run mobile:optimize && npm run build"
	},
